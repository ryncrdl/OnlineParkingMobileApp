<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <androidx.cardview.widget.CardView
        android:id="@+id/doctors_container"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="15dp"
        android:background="@color/white"
        app:cardCornerRadius="20dp"
        app:cardElevation="5dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            >

        <TextView
            android:id="@+id/car_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_margin="15dp"
            android:layout_marginEnd="10dp"
            android:text="Vehicle: "
            android:textColor="@color/black"
            android:textSize="18sp"
            android:textStyle="bold" />

            <TextView
                android:id="@+id/car_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:ellipsize="end"
                android:maxEms="10"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@id/car_label"
                android:layout_marginStart="115dp"
                android:layout_marginTop="15dp"
                android:singleLine="true"
                android:text="Raptorsssssssssssssssssssssssssssssssssss"
                android:textColor="#323232"
                android:textSize="18sp"

                />

        <TextView
            android:id="@+id/timein_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/car_label"
            android:layout_marginStart="15dp"
            android:layout_marginTop="45dp"
            android:layout_marginEnd="10dp"
            android:text="Time in:"
            android:textColor="#323232"
            android:textSize="13sp"
            android:textStyle="bold" />

            <TextView
                android:id="@+id/timein"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/car_label"
                android:layout_marginStart="115dp"
                android:layout_marginTop="45dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@id/timein"
                android:ellipsize="end"
                android:lineHeight="20dp"
                android:maxEms="20"
                android:singleLine="true"
                android:text="06:46:41 pm"
                android:textColor="#323232"
                android:textSize="13sp"

                />

        <TextView
            android:id="@+id/timeout_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/timein"
            android:layout_marginStart="15dp"
            android:layout_marginTop="70dp"
            android:layout_marginEnd="10dp"
            android:text="Time out:"
            android:textColor="#323232"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/timeout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/timein"
            android:layout_marginStart="115dp"
            android:layout_marginTop="70dp"
            android:layout_marginEnd="10dp"
            android:layout_toEndOf="@id/timeout_label"
            android:ellipsize="end"
            android:lineHeight="20dp"
            android:maxEms="20"
            android:singleLine="true"
            android:text="On Parking"
            android:textColor="#323232"
            android:textSize="13sp"

            />

        <TextView
            android:id="@+id/date_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/timeout_label"
            android:layout_marginStart="15dp"
            android:layout_marginTop="95dp"
            android:layout_marginEnd="10dp"
            android:text="Date:"
            android:textColor="#323232"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/timeout"
            android:layout_marginStart="115dp"
            android:layout_marginTop="95dp"
            android:layout_marginEnd="10dp"
            android:layout_toEndOf="@id/date_label"
            android:singleLine="true"
            android:text="2023-10-10"
            android:textColor="#323232"
            android:textSize="13sp"

            />

            <TextView
                android:id="@+id/floor_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/date_label"
                android:layout_marginStart="15dp"
                android:layout_marginTop="120dp"
                android:layout_marginEnd="10dp"
                android:text="Floor:"
                android:textColor="#323232"
                android:textSize="13sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/floor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/date"
                android:layout_marginStart="115dp"
                android:layout_marginTop="120dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@id/floor_label"
                android:singleLine="true"
                android:text="Floor Number"
                android:textColor="#323232"
                android:textSize="13sp"

                />

            <TextView
                android:id="@+id/slot_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/floor_label"
                android:layout_marginStart="15dp"
                android:layout_marginTop="145dp"
                android:layout_marginEnd="10dp"
                android:text="Slot:"
                android:textColor="#323232"
                android:textSize="13sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/slot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/floor"
                android:layout_marginStart="115dp"
                android:layout_marginTop="145dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@id/floor_label"
                android:singleLine="true"
                android:text="Slot Number"
                android:textColor="#323232"
                android:textSize="13sp"

                />
        </FrameLayout>
    </androidx.cardview.widget.CardView>
</RelativeLayout>
